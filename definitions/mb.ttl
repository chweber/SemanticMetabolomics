@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix mbco: <http://www.ipb-halle.de/ontology/mbco#> .

mbco:record a rdfs:Class ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#record> ;
	rdfs:label "Record" ;
	rdfs:comment "A class for massbank records" ;
	rdfs:subClassOf <http://semanticscience.org/resource/SIO_000088> .

mbco:assay a rdfs:Class ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#assay> ;
	rdfs:label "Assay" ;
	rdfs:comment "The assay describing the mass spectrometry experiment that is described in the record" .

mbco:spectrum a rdfs:Class ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#spectrum> ;
	rdfs:label "Spectrum" ;
	rdfs:comment "Information of the spectrum generated by an assay" .
	
mbco:peak a rdfs:Class ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#peak> ;
	rdfs:label "Peak" ;
	rdfs:comment "A peak of the linked spectrum" ;
	rdfs:subClassOf <http://semanticscience.org/resource/SIO_011123> .
	
mbco:chemical_entity a rdfs:Class ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#chemical_entity> ;
	rdfs:label "Chemical entity used for the spectrum" ;
	rdfs:comment "Contains meta-information about the chemical entity of the recorded spectrum" .
	
mbco:has-accession a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-accession> ;
    rdfs:label "Accession number" ;
    rdfs:range xsd:string ;
	rdfs:comment "The accession number of the record in Massbank" .

mbco:hyperlink-record a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#contains_peak> ;
	rdfs:range mbco:string ;
	rdfs:label "Hyperlink to record in Massbank Opendata" .
	
mbco:describes a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#describes> ;
    rdfs:label "The link to the assay described by the record" ;
    rdfs:range mbco:assay ;
	rdfs:comment "The assay described by the record" .

mbco:is-described-by a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#is-described-by> ;
    rdfs:label "The record describing the record" ;
    rdfs:range mbco:record ;
	rdfs:comment "The record describing the record" .

mbco:ms-type a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#ms-type> ;
    rdfs:label "MS Type" ;
    rdfs:range xsd:string ;
	rdfs:comment "The MS-Type with which the assay was generated" .
	
mbco:has-output a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-output> ;
    rdfs:label "Has output" ;
    rdfs:range xsd:spectrum ;
	rdfs:comment "The spectrum generated by the linked assay" .

mbco:is-output-of a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#is-output-of> ;
    rdfs:label "Is output of" ;
    rdfs:range xsd:assay ;
	rdfs:comment "The assay generating the linked assay" .
	
mbco:identifies a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#identifies> ;
    rdfs:label "Identifies" ;
    rdfs:range xsd:chemical_entity ;
	rdfs:comment "The chemical entity the was used to generate the record" .

mbco:name a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#name> ;
    rdfs:label "Name" ;
    rdfs:range xsd:string ;
	rdfs:comment "The name of the chemical entity" .
	
mbco:has-formula a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-formula> ;
    rdfs:label "Formula" ;
    rdfs:range xsd:string ;
	rdfs:comment "The formula of the chemical entity" .
	
mbco:has-smiles a rdf:Property ;
    rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-smiles> ;
    rdfs:label "Smiles" ;
    rdfs:range xsd:string ;
	rdfs:comment "The smiles string describing the formula of the chemical entity" .
	
mbco:instrument a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#instrument> ;
    rdfs:label "Mass spectrometry instrument" ;
    rdfs:range xsd:string ;
	rdfs:comment "The instrument with which the spectrum of the record was created" .	
	
mbco:has-int a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-int> ;
    rdfs:label "The intensity of a peak" ;
    rdfs:range xsd:integer ;
	rdfs:comment "The intensity of a given peak stored as an integer value" .
	
mbco:is-peak-of a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#is-peak-of> ;
	rdfs:range mbco:record ;
	rdfs:label "The link between mbco:peak and mbco:spectrum" .
	
mbco:contains-peak a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#contains-peak> ;
	rdfs:range mbco:peak ;
	rdfs:label "The link between mbco:spectrum and mbco:peak" .
	
mbco:has-mz a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-mz> ;
    rdfs:label "The mass/charge ratio of a peak" ;
    rdfs:range xsd:float ;
	rdfs:comment "The mass/charge ratio of a given peak stored as a float value" .
	
mbco:has-rel-int a rdf:Property ;
	rdfs:isDefinedBy <http://www.ipb-halle.de/ontology/mbco#has-rel-int> ;
    rdfs:label "The relative intensity of a peak" ;
    rdfs:range xsd:integer ;
	rdfs:comment "The relative intensity of a given peak, where the maximum intensity in a spectrum is defined as 999 while all others are put in relationship" .